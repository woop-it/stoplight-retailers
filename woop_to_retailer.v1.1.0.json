          "404": {
            "description": "Le 'orderId' n'a pas été trouvé",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "./retailer_to_woop.v1.4.0.json#/components/schemas/errorCode"
                },
                "examples": {
                  "example": {
                    "value": {
                      "statusCode": 404,
                      "error": "Not Found",
                      "message": "Not Found"
                    }
                  }
                }
              }
            }
          }
        },
        "summary": "Recevoir les mises à jour de statut de livraison"
      }
    },
